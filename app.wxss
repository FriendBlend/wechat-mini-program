/**app.wxss**/
@import "./frontend/fonts/abeezee/abeezee.wxss";

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
/* blue: #10AEFF green: #6CE89E */
.grey-button:active {
  background-color: #b8b8b8; /* rgb(231, 230, 225) */
}
.white-button:active {
  box-shadow: 0px 4px 4px 0px rgba(0, 0, 0, 0.25) inset;
}
